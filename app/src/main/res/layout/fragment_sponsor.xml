<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/fragment_sponsor"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <TextView
                android:id="@+id/textView7"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="0.87"
                android:gravity="center"
                android:padding="@dimen/fragment_sponsor_title_padding"
                android:text="@string/StrategicSponsor_0"
                android:textColor="@color/black"
                android:textSize="@dimen/fragment_sponsor_title_text_size" />

            <GridLayout
                android:id="@+id/fragment_news_row_0"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="0.87"
                android:gravity="center"
                android:padding="@dimen/fragment_sponsor_title_padding"
                android:text="@string/HonorarySponsor_1"
                android:textColor="@color/black"
                android:textSize="@dimen/fragment_sponsor_title_text_size" />

            <GridLayout
                android:id="@+id/fragment_news_row_1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="0.87"
                android:gravity="center"
                android:padding="@dimen/fragment_sponsor_title_padding"
                android:text="@string/Sponsors_2"
                android:textColor="@color/black"
                android:textSize="@dimen/fragment_sponsor_title_text_size" />

            <GridLayout
                android:id="@+id/fragment_news_row_2"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

            <GridLayout
                android:id="@+id/fragment_news_row_4"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:columnCount="@integer/fragment_sponsor_column_count">

            </GridLayout>

            <GridLayout
                android:id="@+id/fragment_news_row_6"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="0.87"
                android:gravity="center"
                android:padding="@dimen/fragment_sponsor_title_padding"
                android:text="@string/Partners_8"
                android:textColor="@color/black"
                android:textSize="@dimen/fragment_sponsor_title_text_size" />

            <GridLayout
                android:id="@+id/fragment_news_row_8"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="0.87"
                android:gravity="center"
                android:padding="@dimen/fragment_sponsor_title_padding"
                android:text="@string/MediaPartners_10"
                android:textColor="@color/black"
                android:textSize="@dimen/fragment_sponsor_title_text_size" />

            <GridLayout
                android:id="@+id/fragment_news_row_10"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

            <TextView
                android:id="@+id/textView12"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:alpha="0.87"
                android:gravity="center"
                android:padding="@dimen/fragment_sponsor_title_padding"
                android:text="@string/AwardSponsors_12"
                android:textColor="@color/black"
                android:textSize="@dimen/fragment_sponsor_title_text_size" />

            <GridLayout
                android:id="@+id/fragment_news_row_12"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_gravity="center"
                android:columnCount="@integer/fragment_sponsor_column_count" />

        </LinearLayout>
    </ScrollView>

</LinearLayout>